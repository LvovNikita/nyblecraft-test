// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model User {
    email String @unique
    password String
    firstName String @db.VarChar(255)
    lastName String @db.VarChar(255)
    image String
    pdf Bytes?
}
